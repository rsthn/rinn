{
    "name": "rinn",
    "version": "2.0.37",
    "description": "Rinn Library",
    "type": "module",
    "main": "main.js",
    "scripts": {
        "test": "echo Ok",
        "requirements": "yarn global add --ignore-optional parcel",
        "deploy": "yarn build && node ./local/deploy.js",
        "build": "parcel build --no-cache --target browser _export.js --no-source-maps"
    },
    "browser": "dist/rinn.js",
    "targets": {
        "browser": {
            "isLibrary": false,
            "engines": {
                "browsers": "since 2020-01"
            },
            "optimize": true
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rsthn/rinn.git"
    },
    "author": "RedStar Technologies",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rsthn/rinn/issues"
    },
    "homepage": "https://github.com/rsthn/rinn#readme",
    "files": [
        "dist/",
        "*.js",
        "LICENSE",
        "README.md"
    ],
    "dependencies": {
        "base-64": "^1.0.0",
        "node-fetch": "^2.6.1"
    }
}